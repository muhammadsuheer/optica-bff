{
  "name": "optica-app",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "bash scripts/start-production.sh",
    "start:nomigrate": "node dist/index.js",
    "migrate": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.2",
    "@prisma/client": "^6.14.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
  "@opentelemetry/api": "^1.8.0",
  "@opentelemetry/sdk-node": "^0.53.0",
  "@opentelemetry/auto-instrumentations-node": "^0.53.0",
  "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.1",
    "envalid": "^8.1.0",
    "graphql-request": "^7.2.0",
    "hono": "^4.9.1",
    "ioredis": "^5.7.0",
    "lru-cache": "^11.1.0",
    "pg": "^8.16.3",
    "prisma": "^6.14.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.2.0",
    "tslib": "^2.8.1",
    "undici": "^7.13.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.10",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.9.2"
  }
}
