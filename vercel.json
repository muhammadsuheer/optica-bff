{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "framework": null,
  "functions": {
    "src/app/edge.ts": {
      "runtime": "edge"
    }
  },
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "/src/app/edge.ts"
    },
    {
      "src": "/health",
      "dest": "/src/app/edge.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app/edge.ts"
    }
  ],
  "headers": [
    {
      "source": "/v1/catalog/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=300"
        },
        {
          "key": "Vary",
          "value": "Accept-Encoding, Authorization"
        }
      ]
    },
    {
      "source": "/v1/commerce/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "private, no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/v1/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "private, no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/v1/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "private, max-age=60"
        }
      ]
    },
    {
      "source": "/health",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=30"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1", "sfo1", "lhr1"],
  "github": {
    "enabled": false
  }
}